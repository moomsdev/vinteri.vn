{
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development node --openssl-legacy-provider ./node_modules/webpack/bin/webpack.js --config resources/build/webpack.development.js",
    "build": "cross-env NODE_ENV=production webpack --config resources/build/webpack.production.js",
    "lint": "run-s -cl lint:wpcs lint:scripts lint:styles",
    "lint-fix": "run-s -cl lint-fix:wpcs lint-fix:scripts lint-fix:styles",
    "lint:wpcs": "./vendor/bin/phpcs --colors --standard=WordPress --ignore=app/src/* app/ theme/",
    "lint:scripts": "eslint --color ./resources/scripts/**",
    "lint:styles": "stylelint --color ./resources/styles/**",
    "lint-fix:wpcs": "./vendor/bin/phpcbf --colors --standard=WordPress --ignore=app/src/* app/ theme/",
    "lint-fix:scripts": "eslint --color --fix ./resources/scripts/**",
    "lint-fix:styles": "stylelint --color --fix ./resources/styles/**",
    "i18n": "run-s -cl i18n:textdomain i18n:pot",
    "i18n:textdomain": "wpi18n addtextdomain --exclude=dist,docs,node_modules,resources,vendor --textdomain=app",
    "i18n:pot": "wpi18n makepot --exclude=dist,docs,node_modules,resources,vendor --main-file=theme/style.css --domain-path=languages/ --pot-file=app.pot --type=wp-theme --textdomain=app",
    "release": "run-s -cl build release:zip",
    "release:zip": "node resources/build/release/release.js"
  },
  "browserslist": [
    "last 3 versions"
  ],
  "dependencies": {
    "@csstools/normalize.css": "^12.1.1",
    "@fancyapps/fancybox": "^3.5.7",
    "airbnb-browser-shims": "^2.1.0",
    "aos": "^3.0.0-beta.6",
    "autoprefixer": "^10.4.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "^5.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.0",
    "css-loader": "^6.5.0",
    "file-loader": "^6.2.0",
    "gsap": "^3.12.5",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-webpack-plugin": "^2.4.0",
    "import-glob": "^1.5.0",
    "jquery": "^3.7.1",
    "jquery-pjax": "latest",
    "jquery-validation": "^1.19.3",
    "loader-utils": "^2.0.0",
    "mburger-css": "^1.3.3",
    "mmenu-js": "^8.5.20",
    "node-gyp": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.16.1",
    "postcss-loader": "^5.0.0",
    "sass": "^1.78.0",
    "sass-loader": "^12.0.0",
    "select2": "^4.1.0-rc.0",
    "skeleton-screen-css": "^1.1.0",
    "style-loader": "^3.3.1",
    "sweetalert2": "^11.7.0",
    "swiper": "^6.3.5",
    "swup": "^4.7.0",
    "webpack-manifest-plugin": "^5",
    "webpack-spritesmith": "^0.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "archiver": "^3.0.0",
    "browser-sync": "^2.27.7",
    "browser-sync-webpack-plugin": "^2.3.0",
    "css-minimizer-webpack-plugin": "^3.0.0",
    "cssnano": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "mini-css-extract-plugin": "^2.9.1",
    "node-wp-i18n": "^1.2.1",
    "postcss": "^8.4.18",
    "shelljs": "^0.8.5",
    "stylelint": "^14.0.0",
    "stylelint-config-recommended-scss": "^5.0.1",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.94.0",
    "webpack-assets-manifest": "^5.2.1",
    "webpack-cli": "^4.10.0"
  }
}
