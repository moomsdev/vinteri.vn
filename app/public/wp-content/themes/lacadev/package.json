{
  "name": "lacadev-theme",
  "version": "3.0.0",
  "description": "WordPress theme for La CÃ  Dev",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=20.0"
  },
  "scripts": {
    "dev": "concurrently \"yarn dev:theme\" \"yarn dev:blocks\"",
    "dev:theme": "cross-env NODE_ENV=development webpack --color --config resources/build/webpack.development.js",
    "dev:blocks": "wp-scripts start block-gutenberg/index.js --output-path=./dist/gutenberg",
    "build": "yarn build:theme && yarn build:blocks",
    "build:theme": "cross-env NODE_ENV=production webpack --color --config resources/build/webpack.production.js",
    "build:blocks": "wp-scripts build block-gutenberg/index.js --output-path=./dist/gutenberg",
    "lint": "run-s -cl lint:scripts lint:styles",
    "lint:scripts": "wp-scripts lint-js 'resources/scripts' 'block-gutenberg'",
    "lint:styles": "stylelint 'resources/styles/**/*.{css,scss}'",
    "lint-fix": "run-s -cl lint-fix:scripts lint-fix:styles",
    "lint-fix:scripts": "wp-scripts lint-js --fix 'resources/scripts' 'block-gutenberg'",
    "lint-fix:styles": "stylelint --fix 'resources/styles/**/*.{css,scss}'"
  },
  "browserslist": [
    "last 3 versions"
  ],
  "dependencies": {
    "@10up/block-components": "^1.15.9",
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.21.0",
    "@csstools/normalize.css": "^12.1.1",
    "@wordpress/block-editor": "^12.0.0",
    "@wordpress/blocks": "^12.0.0",
    "@wordpress/components": "^25.0.0",
    "@wordpress/element": "^5.0.0",
    "@wordpress/i18n": "^4.0.0",
    "@wordpress/scripts": "^27.0.0",
    "aos": "^3.0.0-beta.6",
    "autoprefixer": "^10.4.17",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "gsap": "^3.12.5",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "import-glob": "^1.5.0",
    "loader-utils": "^3.2.1",
    "mburger-css": "^1.3.3",
    "mini-css-extract-plugin": "^2.8.0",
